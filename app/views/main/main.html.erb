<h2>List of Students</h2>

<table>
  <tr>
    <th>Name</th>
    <th>MWF,7-8:30</th>
    <th>MWF,8:30-10</th>
    <th>MWF,10-12</th>
    <th>TTh,7-8:30</th>
    <th>TTh,8:30-10</th>
    <th>TTh,10-12</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @skeds.each do |sked| %>
  <tr>
    <td><%= sked.name %></td>
    <td><%= sked.mwf1 %></td>
    <td><%= sked.mwf2 %></td>
    <td><%= sked.mwf3 %></td>
    <td><%= sked.tth1 %></td>
    <td><%= sked.tth2 %></td>
    <td><%= sked.tth3 %></td>
    <td><%= link_to 'Edit', edit_sked_path(sked) %></td>
    <td><%= link_to 'Destroy', sked, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<center><%= link_to 'New Sked', new_sked_path %></center>
