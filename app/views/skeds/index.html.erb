<h1>Listing skeds</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Mwf1</th>
    <th>Mwf2</th>
    <th>Mwf3</th>
    <th>Tth1</th>
    <th>Tth2</th>
    <th>Tth3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @skeds.each do |sked| %>
  <tr>
    <td><%= sked.name %></td>
    <td><%= sked.mwf1 %></td>
    <td><%= sked.mwf2 %></td>
    <td><%= sked.mwf3 %></td>
    <td><%= sked.tth1 %></td>
    <td><%= sked.tth2 %></td>
    <td><%= sked.tth3 %></td>
    <td><%= link_to 'Show', sked %></td>
    <td><%= link_to 'Edit', edit_sked_path(sked) %></td>
    <td><%= link_to 'Destroy', sked, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sked', new_sked_path %>
